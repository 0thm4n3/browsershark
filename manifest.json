{
   
	"name": "Browsershark",
    
	"homepage_url": "http://www.browsershark.org",
	"version": "1.0.1",
	"manifest_version" : 2,
   
	"description": "Browsershark is a forensics tool.",
   
	"browser_action": {
		"default_icon": "icon.png"
	},
	
	"icons": 
	{
		"128": "icon.png",
		"19": "icon.png",
		"48": "icon.png"
	},
   
	"options_page": "options.html",
   
	"background": {
		"scripts": ["background.js", "jquery.min.js"]
	},
   
	"permissions": 
	[ 
		"tabs",
		"*://*/*",
		"notifications",
        "webRequestBlocking",
        "webRequest"
	]

}
